<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.example.hong.simplehomeworklist"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".ExplainCal"/>
        <activity android:name=".ExplainTime"/>
        <activity android:name=".ExplainTip"/>
        <activity android:name=".HomeworkList"/>

        <activity android:name=".TipCalculator"/>
        <activity android:name=".SimpleCalculator"/>
        <activity android:name=".TimeTable"/>
        <activity android:name=".ChangePwd"/>
    </application>

</manifest>
